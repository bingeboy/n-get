{
  "name": "n-get",
  "version": "1.4.0",
  "homepage": "https://bingeboy.github.io/n-get/",
  "description": "Wget like CLI Tool done with Node.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bingeboy/n-get.git"
  },
  "bugs": {
    "url": "https://github.com/bingeboy/n-get/issues"
  },
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "NODE_ENV=test mocha test/*.js --timeout 30000",
    "test:watch": "NODE_ENV=test mocha test/*.js --watch --timeout 30000",
    "test:coverage": "NODE_ENV=test nyc mocha test/*.js --timeout 30000",
    "lint": "eslint . && echo '✓ No linting errors found!'",
    "lint:verbose": "eslint . --debug 2>&1 | grep -E 'files found|Processing' && eslint . && echo '✓ No linting errors found!'",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf temp/* test/temp/* test/cli-test/*"
  },
  "author": "bingeboy",
  "license": "MIT",
  "dependencies": {
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "progress": "^2.0.3",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^10.0.3"
  },
  "preferGlobal": "true",
  "bin": {
    "nget": "./index.js"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "eslint": "^9.31.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0"
  }
}
