{
  "name": "n-get",
  "version": "1.2.0",
  "homepage": "https://github.com/bingeboy/n-get",
  "description": "Wget like CLI Tool done with Node.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bingeboy/n-get.git"
  },
  "bugs": {
    "url": "https://github.com/bingeboy/n-get/issues"
  },
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "mocha test/*.js --timeout 30000",
    "test:watch": "mocha test/*.js --watch --timeout 30000",
    "test:coverage": "nyc mocha test/*.js --timeout 30000",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "clean": "rm -rf temp/* test/temp/* test/cli-test/*"
  },
  "xo": {
    "space": 4,
    "rules": {
      "unicorn/no-process-exit": "off",
      "unicorn/prevent-abbreviations": ["error", {
        "allowList": {
          "args": true,
          "argv": true,
          "env": true,
          "err": true,
          "fn": true,
          "req": true,
          "res": true,
          "tmp": true,
          "dir": true,
          "dest": true,
          "src": true,
          "lib": true,
          "opts": true,
          "stats": true,
          "url": true,
          "urls": true,
          "uri": true,
          "ui": true,
          "i": true
        }
      }],
      "unicorn/filename-case": ["error", {
        "cases": {
          "kebabCase": true,
          "camelCase": true,
          "pascalCase": true
        }
      }],
      "unicorn/no-array-reduce": "off",
      "unicorn/no-null": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prefer-top-level-await": "off",
      "unicorn/import-style": "off",
      "unicorn/prefer-node-protocol": "off",
      "unicorn/numeric-separators-style": "off",
      "unicorn/no-anonymous-default-export": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-callback-reference": "off",
      "no-console": "off"
    },
    "ignores": [
      "temp/**",
      "test-resume/**", 
      "gh_pages/**",
      "*.tgz",
      "test/temp/**"
    ]
  },
  "author": "bingeboy",
  "license": "MIT",
  "dependencies": {
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "progress": "^2.0.3",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^10.0.3"
  },
  "preferGlobal": "true",
  "bin": {
    "nget": "./index.js"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "eslint": "^9.31.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "xo": "^1.2.1"
  }
}
